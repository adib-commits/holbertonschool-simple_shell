#include <stdio.h>
#include <stdlib.h

/**
 * @list : prints it on the next line
 *
 * Return : Always 0
 */

main(void)
{
char *line = NULL;
size_t len = 0;
ssize_t nread;

while (1)
{
printf("$ ");
nread = getline(&line, &len, stdin);

if (nread == -1)
{
printf("\n");
break;
}

printf("%s", line);
}

free(line);
return (0);
}
